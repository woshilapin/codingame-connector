{
  "name": "codingame-connector",
  "version": "0.4.3",
  "description": "An interface for Codingame website",
  "keywords": [
    "codingame",
    "watch"
  ],
  "license": "GPL-3.0",
  "homepage": "https://github.com/woshilapin/codingame-connector",
  "author": "woshilapin <woshilapin@tuziwo.info>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/woshilapin/codingame-connector.git"
  },
  "bugs": {
    "url": "https://github.com/woshilapin/codingame-connector/issues"
  },
  "main": "src/codingame-api.js",
  "scripts": {
    "start": "babel-node --presets es2015-node-auto,stage-3 -- src/watch.js",
    "lint": "eslint --cache --color src/*",
    "doc": "rimraf ./doc/* && jsdoc --configure .jsdocrc src/",
    "build": "rimraf ./lib/* && babel src --out-dir lib",
    "codecov": "nyc report --reporter=lcovonly && codecov",
    "changelog": "changelog codingame-connector all -m > CHANGELOG.md",
    "test": "nyc mocha",
    "prepublish": "npm run build && npm run changelog"
  },
  "bin": {
    "cg-watch": "./bin/cg-watch.patch.sh"
  },
  "dependencies": {
    "ansi-escapes": "^1.4.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "babel-runtime": "^6.11.6",
    "colors": "^1.1.2",
    "commander": "^2.9.0",
    "request": "^2.75.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.0",
    "babel-preset-es2015-node-auto": "0.0.5",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "changelog": "^1.0.7",
    "codecov": "^1.0.1",
    "eslint": "^3.8.1",
    "jsdoc": "^3.4.2",
    "jsdoc-babel": "^0.2.1",
    "mocha": "^3.1.2",
    "mock-fs": "^3.12.1",
    "nock": "^9.0.2",
    "nyc": "^8.3.2",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  }
}
